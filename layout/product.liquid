</script><script>document.addEventListener('DOMContentLoaded', function() {
    const productRecommendationsSection = document.querySelector('.product-recommendations');
    const url = productRecommendationsSection.dataset.url;
    fetch(url)
      .then(response => response.text())
      .then(text => {
      const html = document.createElement('div');
      html.innerHTML = text;
      const recommendations = html.querySelector('.product-recommendations');
      if (recommendations && recommendations.innerHTML.trim().length) {
        productRecommendationsSection.innerHTML = recommendations.innerHTML;
      }
    })
      .catch(e => {
      console.error(e);
    });
  });
</script><script>document.addEventListener('DOMContentLoaded', function() {
    // Find the variant container
    const container = document.querySelector('[li-element="product-variant-container"]');
    if (!container) return;
    // Add a click handler to the container
    container.addEventListener('click', function(event) {
      // Short delay to let Alpine.js finish processing
      setTimeout(function() {
        // Update styles directly using inline styles
        container.querySelectorAll('input[type="radio"]').forEach(function(radio) {
          const label = radio.parentElement.querySelector('.product-variant-picker-button');
          if (label) {
            if (radio.checked) {
              label.style.color = '#161616';
              label.style.fontWeight = '600';
            } else {
              label.style.color = '';
              label.style.fontWeight = '';
            }
          }
        });
      }, 50);
    });
    // Run once on page load with a delay
    setTimeout(function() {
      container.click(); // Trigger the click handler
    }, 500);
  });
</script>
